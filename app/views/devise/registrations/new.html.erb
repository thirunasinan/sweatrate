  <div class="forget-password-screen">
    <div class="app-title">
      <h2>Sign Up</h2>
    </div>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="input-group form-group">
        <span class="input-group-addon"><i class="fa fa-user"></i></span>
        <%= f.text_field :name, class: 'form-control', placeholder: 'Full Name' %>
      </div>

      <div class="input-group form-group">
        <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
        <%= f.email_field :email, class: 'form-control', placeholder: 'Email Id' %>
      </div>

      <div class="input-group form-group">
        <span class="input-group-addon password-group"><i class="fa fa-lock"></i></span>
        <%= f.password_field :password, autocomplete: "off", class: 'form-control', placeholder: 'Password' %>
        <% if @validatable %>
          <span class="help-block"><%= @minimum_password_length %> characters minimum</span>
        <% end %>
      </div>

      <div class="input-group form-group">
        <span class="input-group-addon password-group"><i class="fa fa-lock"></i></span>
        <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control', placeholder: 'Confirmation Password' %>
      </div>
      <div class="actions">
        <%= button_tag(type: 'submit', class: "btn btn-primary") do %>
           Sign up <i class="fa fa-send"></i>
        <% end %>
      </div>
    <% end %>
    <div class="redirect-action first">
      <p></p>
    </div>
   <div class="redirect-action ">
      <p>Existing User? Welcome.  <%= link_to "SIGNIN HERE", new_session_path(resource_name) %></p>
    </div>
  </div>
